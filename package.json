{
  "name": "developers-quiz",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "NODE_ENV=production tsx server/index.ts",
    "format": "prettier --write",
    "start": "NODE_ENV=production tsx server/index.ts",
    "questions:to-json": "tsx scripts/questions-convert.ts --to-json --input questions.md --output src/data/questions.json",
    "questions:to-md": "tsx scripts/questions-convert.ts --to-md --input src/data/questions.json --output questions.md"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "dotenv": "^16.6.1",
    "form-data": "^4.0.1",
    "mailgun.js": "^11.1.0",
    "react": "^19.2.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^22.13.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "^3.8.1",
    "tsx": "^4.20.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.2.4"
  }
}
